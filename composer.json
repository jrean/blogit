{
    "name": "jrean/blogit",
    "description": "Blog with Git(hub). For Laravel 5.*. Fetch and Parse Documents hosted on Github.",
    "keywords": ["laravel", "framework", "markdown", "blog", "Blog", "Laravel Blog", "laravel blog", "Git", "Github", "git", "github"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jean Ragouin",
            "email": "go@askjong.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.0.*|5.1.*|5.2.*",
        "knplabs/github-api": "~1.4",
        "erusev/parsedown-extra": "^0.7.1",
        "symfony/yaml": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "Jrean\\Blogit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jrean\\Tests\\Blogit\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
